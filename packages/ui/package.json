{
  "name": "@app/ui",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "test": "jest --passWithNoTests",
    "tsc": "tsc --noEmit",
    "postinstall": "pnpm --silent setup:dsfr",
    "with-env": "dotenv -e ../../.env --",
    "setup:dsfr": "FROM=\"./node_modules/@gouvfr/dsfr/dist\"; TO=\"public/dsfr\"; if [ ! -d \"$FROM\" ]; then FROM=\"../../node_modules/@gouvfr/dsfr/dist\"; fi; mkdir -p $TO/utility && cp $FROM/dsfr.module.min.js $TO/ && cp $FROM/dsfr.module.min.js.map $TO/ && cp $FROM/dsfr.nomodule.min.js $TO/ && cp $FROM/dsfr.nomodule.min.js.map $TO/ && cp $FROM/dsfr.min.css $TO/ && cp $FROM/utility/utility.min.css $TO/utility/ && cp -R $FROM/icons $TO/ && cp -R $FROM/artwork $TO/ && cp -R $FROM/fonts $TO/",
    "cy": "pnpm --silent with-env cypress",
    "cy:open": "pnpm --silent with-env cypress open"
  },
  "engines": {
    "node": "^18.16"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^0.63.1",
    "@gouvfr/dsfr": "^1.9.3",
    "@hookform/resolvers": "^3.1.1",
    "@tiptap/core": "^2.0.3",
    "@tiptap/extension-link": "^2.0.3",
    "@tiptap/pm": "^2.0.3",
    "@tiptap/react": "^2.0.3",
    "@tiptap/starter-kit": "^2.0.3",
    "classnames": "^2.3.2",
    "dotenv": "^16.1.4",
    "next": "^13.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "tsafe": "^1.6.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.0.20",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.18",
    "@types/react": "^18.2.12",
    "@types/testing-library__jest-dom": "^5.14.6",
    "cypress": "^12.14.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "typescript": "^5.0.4"
  }
}
