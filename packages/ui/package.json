{
  "name": "@app/ui",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "test": "jest --passWithNoTests",
    "tsc": "tsc --noEmit",
    "project-postinstall": "pnpm --silent setup:dsfr",
    "with-env": "dotenv -e ../../.env --",
    "setup:dsfr": "FROM=\"./node_modules/@gouvfr/dsfr/dist\"; TO=\"public/dsfr\"; if [ ! -d \"$FROM\" ]; then FROM=\"../../node_modules/@gouvfr/dsfr/dist\"; fi; mkdir -p $TO/utility && cp $FROM/dsfr.module.min.js $TO/ && cp $FROM/dsfr.module.min.js.map $TO/ && cp $FROM/dsfr.nomodule.min.js $TO/ && cp $FROM/dsfr.nomodule.min.js.map $TO/ && cp $FROM/dsfr.min.css $TO/ && cp $FROM/utility/utility.min.css $TO/utility/ && cp -R $FROM/icons $TO/ && cp -R $FROM/artwork $TO/ && cp -R $FROM/fonts $TO/",
    "cy": "pnpm --silent with-env cypress",
    "cy:open": "pnpm --silent with-env cypress open"
  },
  "engines": {
    "node": "^20.3"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^0.63.2",
    "@gouvfr/dsfr": "^1.9.3",
    "@hookform/resolvers": "^3.1.1",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "next": "^13.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.0.23",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@types/testing-library__jest-dom": "^5.14.6",
    "cypress": "^12.15.0",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "typescript": "^5.0.4"
  }
}
