{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "prisma generate": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": true
    },
    "dev": {
      "dependsOn": [
        "^prisma generate"
      ],
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build",
        "^prisma generate"
      ],
      "outputs": [
        ".next/**"
      ],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "tsc": {
      "cache": false
    }
  },
  "globalEnv": [
    "ANALYZE",
    "CI",
    "CHROMATIC_APP_ID",
    "DATABASE_URL",
    "BRANCH",
    "CDK_FORCE_BRANCH",
    "COCKPIT_GRAPHANA_EDITORS",
    "COCKPIT_GRAPHANA_VIEWERS",
    "COCKPIT_METRICS_URL",
    "COCKPIT_LOGS_URL",
    "COCKPIT_ALERT_MANAGER_URL",
    "COCKPIT_GRAFANA_URL",
    "CYPRESS_BASE_URL",
    "NAMESPACE",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NODE_ENV",
    "NEXT_RUNTIME",
    "PORT",
    "APP",
    "NEXT_PUBLIC_APP_NAME",
    "NEXT_PUBLIC_APP_SLUG",
    "GITHUB_TOKEN",
    "PRODUCTION_URL",
    "PRODUCTION_APP",
    "BASE_URL",
    "NEXT_PUBLIC_SIREN_API_KEY",
    "NEXT_PUBLIC_CONTACT_EMAIL",
    "SIREN_ACCESS_TOKEN",
    "SCW_DEFAULT_REGION",
    "SCW_SECRET_KEY",
    "SCW_ACCESS_KEY",
    "SCW_API_KEY_ID",
    "SCW_API_KEY_SECRET",
    "UPLOADS_BUCKET",
    "S3_HOST",
    "SCW_ORGANIZATION_ID",
    "SCW_PROJECT_ID",
    "STACK_WEB_IMAGE",
    "SENTRY_AUTH_TOKEN",
    "SENTRY_ENABLE_RELEASE",
    "NEXT_PUBLIC_SENTRY_DSN",
    "NEXT_PUBLIC_SENTRY_ENVIRONMENT",
    "NEXT_PUBLIC_DEVELOPMENT_DISABLE_LINK_PREFETCH",
    "NEXT_PUBLIC_MATOMO_HOST",
    "NEXT_PUBLIC_MATOMO_SITE_ID",
    "PROCONNECT_PREVIEW_HOSTNAME",
    "PROCONNECT_PREVIEW_CLIENT_ID",
    "PROCONNECT_PREVIEW_CLIENT_SECRET",
    "PROCONNECT_MAIN_HOSTNAME",
    "PROCONNECT_MAIN_CLIENT_ID",
    "PROCONNECT_MAIN_CLIENT_SECRET",
    "NEXT_PUBLIC_PROCONNECT_HOSTNAME",
    "NEXT_PUBLIC_PROCONNECT_CLIENT_ID",
    "PROCONNECT_CLIENT_SECRET",
    "PROCONNECT_TEST_USER_EMAIL",
    "PROCONNECT_TEST_USER_PASSWORD",
    "IS_E2E",
    "EMAIL_FROM_ADDRESS",
    "EMAIL_FROM_NAME",
    "SMTP_USERNAME",
    "SMTP_PASSWORD",
    "SMTP_SERVER",
    "SMTP_PORT",
    "CLI_TARGET_DEPLOYMENT_BRANCH",
    "CLI_TARGET_DEPLOYMENT_DATABASE_URL",
    "DATABASE_INSTANCE_NAME",
    "CONTAINER_NAMESPACE_NAME",
    "MAIN_ROOT_DOMAIN",
    "MAIN_SUBDOMAIN",
    "PREVIEW_ROOT_DOMAIN",
    "PREVIEW_SUBDOMAIN",
    "NEXT_TELEMETRY_DISABLED",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_URL",
    "INTERNAL_API_PRIVATE_KEY",
    "DATABASE_INSTANCE_ID",
    "BACKUP_DATABASE_NAME",
    "INTERNAL_API_PRIVATE_KEY",
    "API_ENTREPRISE_TOKEN",
    "CONSEILLER_NUMERIQUE_MONGODB_URL",
    "SUDO_USURPATION",
    "RDV_API_KEY",
    "PRISMA_ENABLE_LOGGING"
  ]
}
