    "node-html-parser": "^6.1.5",
    "nodemailer": "^6.9.1",
    "prisma": "^4.13.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.43.9",
    "react-intersection-observer": "^9.4.3",
    "react-loader-spinner": "^5.3.4",
    "react-swipeable": "^7.0.0",
    "react-textarea-autosize": "8.4.1",
